runhex:

rem *** Hex dumper ***

cls

print "Enter a filename to make a hex dump from:"
input $1

x = RAMSTART

load $1 x
if r = 1 then goto hexerror

hexloop:
  peek a x
  print hex a ;
  print "  " ;
  x = x + 1
  s = s - 1
  if s = 0 then goto hexfinish
  goto hexloop

