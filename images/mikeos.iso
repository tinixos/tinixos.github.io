rem *** draw_box routine ***
rem *** Takes coords in A, B, C and D vars. Uses Z internally ***
draw_box:
  move a b
  for z = a to c
    print "-" ;
  next z
  move a d
  for z = a to c
    print "-" ;
  next z
  for z = b to d
    move a z
    print "|" ;
  next z
  for z = b to d
    move c z
    print "|" ;
  next z
  move a b
  print "+" ;
  move a d
  print "+" ;
  move c b
  print "+" ;
  move c d
  print "+" ;
