  peekint v 65418
  move 22 12
  print $8
  move 22 14
  print "Press escape to end program."
  move 22 15
  print "Press any other key to continue."
  cursor on
  move 53 15
  waitkey j
  if j = 27 then gosub endprog
  gosub closebox
return

