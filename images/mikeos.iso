cserial:
  gosub savevar
  v = 0
  serial rec x
  if x = 4 then v = 1
  if x = 5 then v = 2
  if v > 0 then goto cserialc
  serial send 5
  serial rec x
  if x > 31 then x = 5
  if x = 6 then x = 3
  if x = 0 then v = 6
