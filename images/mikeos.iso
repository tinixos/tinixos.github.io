go_location:
  t = 2
  $5 = "Goto - Location"
  $6 = "Enter a hexdecimal memory address to|go to between 0000 and FFFF."
  v = 1
  gosub inpbox
  $4 = $6
  
  if $4 = "" then return
  gosub highlight_off
  gosub hexstr_to_num
  w = d - f
  d = v
  v = v - w
  f = v
  gosub update_screen
  gosub highlight_on
return

